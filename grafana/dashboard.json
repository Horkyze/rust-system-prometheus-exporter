{
  "__inputs": [
    {
      "name": "DS_PROMETHEUS",
      "label": "Prometheus",
      "description": "Prometheus data source for sysmetrics-rs",
      "type": "datasource",
      "pluginId": "prometheus",
      "pluginName": "Prometheus"
    }
  ],
  "__requires": [
    {
      "type": "grafana",
      "id": "grafana",
      "name": "Grafana",
      "version": "9.0.0"
    },
    {
      "type": "datasource",
      "id": "prometheus",
      "name": "Prometheus",
      "version": "1.0.0"
    },
    {
      "type": "panel",
      "id": "timeseries",
      "name": "Time series",
      "version": "1.0.0"
    },
    {
      "type": "panel",
      "id": "stat",
      "name": "Stat",
      "version": "1.0.0"
    },
    {
      "type": "panel",
      "id": "gauge",
      "name": "Gauge",
      "version": "1.0.0"
    },
    {
      "type": "panel",
      "id": "table",
      "name": "Table",
      "version": "1.0.0"
    },
    {
      "type": "panel",
      "id": "piechart",
      "name": "Pie chart",
      "version": "1.0.0"
    }
  ],
  "id": null,
  "uid": "sysmetrics-rs",
  "title": "sysmetrics-rs System Metrics",
  "description": "Comprehensive Linux system metrics dashboard for the sysmetrics-rs Prometheus exporter. Covers CPU, memory, disk I/O, network, and exporter health.",
  "tags": ["sysmetrics", "prometheus", "linux", "system"],
  "timezone": "browser",
  "editable": true,
  "graphTooltip": 1,
  "fiscalYearStartMonth": 0,
  "liveNow": false,
  "refresh": "30s",
  "schemaVersion": 38,
  "style": "dark",
  "time": {
    "from": "now-1h",
    "to": "now"
  },
  "timepicker": {
    "refresh_intervals": ["5s", "10s", "30s", "1m", "5m"],
    "time_options": ["5m", "15m", "1h", "6h", "12h", "24h", "2d", "7d", "30d"]
  },
  "templating": {
    "list": [
      {
        "name": "datasource",
        "type": "datasource",
        "query": "prometheus",
        "current": {},
        "hide": 0,
        "includeAll": false,
        "multi": false,
        "options": [],
        "refresh": 1,
        "regex": "",
        "skipUrlSync": false
      },
      {
        "name": "instance",
        "type": "query",
        "datasource": { "type": "prometheus", "uid": "${datasource}" },
        "query": "label_values(sysmetrics_build_info, instance)",
        "current": {},
        "hide": 0,
        "includeAll": true,
        "multi": true,
        "options": [],
        "refresh": 2,
        "regex": "",
        "sort": 1,
        "skipUrlSync": false,
        "allValue": ".*"
      },
      {
        "name": "cpu",
        "type": "query",
        "datasource": { "type": "prometheus", "uid": "${datasource}" },
        "query": "label_values(sysmetrics_cpu_seconds_total{instance=~\"$instance\"}, cpu)",
        "current": {},
        "hide": 0,
        "includeAll": true,
        "multi": true,
        "options": [],
        "refresh": 2,
        "regex": "",
        "sort": 3,
        "skipUrlSync": false
      },
      {
        "name": "device",
        "type": "query",
        "datasource": { "type": "prometheus", "uid": "${datasource}" },
        "query": "label_values(sysmetrics_disk_reads_completed_total{instance=~\"$instance\"}, device)",
        "current": {},
        "hide": 0,
        "includeAll": true,
        "multi": true,
        "options": [],
        "refresh": 2,
        "regex": "",
        "sort": 1,
        "skipUrlSync": false
      },
      {
        "name": "interface",
        "type": "query",
        "datasource": { "type": "prometheus", "uid": "${datasource}" },
        "query": "label_values(sysmetrics_network_receive_bytes_total{instance=~\"$instance\"}, interface)",
        "current": {},
        "hide": 0,
        "includeAll": true,
        "multi": true,
        "options": [],
        "refresh": 2,
        "regex": "",
        "sort": 1,
        "skipUrlSync": false
      }
    ]
  },
  "annotations": {
    "list": [
      {
        "builtIn": 1,
        "datasource": { "type": "grafana", "uid": "-- Grafana --" },
        "enable": true,
        "hide": true,
        "iconColor": "rgba(0, 211, 255, 1)",
        "name": "Annotations & Alerts",
        "type": "dashboard"
      }
    ]
  },
  "panels": [
    {
      "collapsed": false,
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 0 },
      "id": 100,
      "title": "Exporter Overview",
      "type": "row"
    },
    {
      "id": 1,
      "title": "Build Info",
      "type": "stat",
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "gridPos": { "h": 4, "w": 4, "x": 0, "y": 1 },
      "fieldConfig": {
        "defaults": {
          "mappings": [],
          "thresholds": { "mode": "absolute", "steps": [{ "color": "green", "value": null }] },
          "color": { "mode": "fixed", "fixedColor": "text" }
        },
        "overrides": []
      },
      "options": {
        "reduceOptions": { "calcs": ["lastNotNull"], "fields": "/^version$/", "values": false },
        "orientation": "auto",
        "textMode": "value",
        "colorMode": "background",
        "graphMode": "none",
        "justifyMode": "center"
      },
      "targets": [
        {
          "expr": "sysmetrics_build_info{instance=~\"$instance\"}",
          "legendFormat": "{{version}}",
          "refId": "A",
          "format": "table",
          "instant": true
        }
      ]
    },
    {
      "id": 2,
      "title": "CPU Count",
      "type": "stat",
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "gridPos": { "h": 4, "w": 4, "x": 4, "y": 1 },
      "fieldConfig": {
        "defaults": {
          "mappings": [],
          "thresholds": { "mode": "absolute", "steps": [{ "color": "blue", "value": null }] },
          "color": { "mode": "thresholds" }
        },
        "overrides": []
      },
      "options": {
        "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false },
        "orientation": "auto",
        "textMode": "auto",
        "colorMode": "value",
        "graphMode": "none",
        "justifyMode": "center"
      },
      "targets": [
        {
          "expr": "sysmetrics_cpu_count{instance=~\"$instance\"}",
          "legendFormat": "{{instance}}",
          "refId": "A",
          "instant": true
        }
      ]
    },
    {
      "id": 3,
      "title": "Total Memory",
      "type": "stat",
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "gridPos": { "h": 4, "w": 4, "x": 8, "y": 1 },
      "fieldConfig": {
        "defaults": {
          "mappings": [],
          "thresholds": { "mode": "absolute", "steps": [{ "color": "purple", "value": null }] },
          "unit": "bytes",
          "color": { "mode": "thresholds" }
        },
        "overrides": []
      },
      "options": {
        "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false },
        "orientation": "auto",
        "textMode": "auto",
        "colorMode": "value",
        "graphMode": "none",
        "justifyMode": "center"
      },
      "targets": [
        {
          "expr": "sysmetrics_memory_total_bytes{instance=~\"$instance\"}",
          "legendFormat": "{{instance}}",
          "refId": "A",
          "instant": true
        }
      ]
    },
    {
      "id": 4,
      "title": "Collector Health",
      "type": "stat",
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "gridPos": { "h": 4, "w": 6, "x": 12, "y": 1 },
      "fieldConfig": {
        "defaults": {
          "mappings": [
            { "type": "value", "options": { "1": { "text": "OK", "color": "green" }, "0": { "text": "FAIL", "color": "red" } } }
          ],
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "red", "value": null },
              { "color": "green", "value": 1 }
            ]
          },
          "color": { "mode": "thresholds" }
        },
        "overrides": []
      },
      "options": {
        "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false },
        "orientation": "horizontal",
        "textMode": "auto",
        "colorMode": "background",
        "graphMode": "none",
        "justifyMode": "center"
      },
      "targets": [
        {
          "expr": "sysmetrics_collector_success{instance=~\"$instance\"}",
          "legendFormat": "{{collector}}",
          "refId": "A",
          "instant": true
        }
      ]
    },
    {
      "id": 5,
      "title": "Total Scrape Duration",
      "type": "timeseries",
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "gridPos": { "h": 4, "w": 6, "x": 18, "y": 1 },
      "fieldConfig": {
        "defaults": {
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "smooth",
            "fillOpacity": 20,
            "pointSize": 5,
            "showPoints": "auto",
            "spanNulls": false,
            "lineWidth": 2
          },
          "unit": "s",
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 0.5 },
              { "color": "red", "value": 1 }
            ]
          },
          "color": { "mode": "palette-classic" }
        },
        "overrides": []
      },
      "options": {
        "tooltip": { "mode": "multi", "sort": "desc" },
        "legend": { "displayMode": "list", "placement": "bottom" }
      },
      "targets": [
        {
          "expr": "sysmetrics_scrape_duration_seconds_total{instance=~\"$instance\"}",
          "legendFormat": "total - {{instance}}",
          "refId": "A"
        }
      ]
    },
    {
      "id": 6,
      "title": "Per-Collector Scrape Duration",
      "type": "timeseries",
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "gridPos": { "h": 6, "w": 12, "x": 0, "y": 5 },
      "fieldConfig": {
        "defaults": {
          "custom": {
            "drawStyle": "bars",
            "fillOpacity": 80,
            "pointSize": 5,
            "showPoints": "never",
            "stacking": { "mode": "normal", "group": "A" },
            "lineWidth": 1
          },
          "unit": "s",
          "color": { "mode": "palette-classic" }
        },
        "overrides": []
      },
      "options": {
        "tooltip": { "mode": "multi", "sort": "desc" },
        "legend": { "displayMode": "list", "placement": "bottom" }
      },
      "targets": [
        {
          "expr": "sysmetrics_scrape_duration_seconds{instance=~\"$instance\"}",
          "legendFormat": "{{collector}}",
          "refId": "A"
        }
      ]
    },
    {
      "id": 7,
      "title": "Collector Errors (Total)",
      "type": "timeseries",
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "gridPos": { "h": 6, "w": 12, "x": 12, "y": 5 },
      "fieldConfig": {
        "defaults": {
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "smooth",
            "fillOpacity": 10,
            "pointSize": 5,
            "showPoints": "auto",
            "spanNulls": false,
            "lineWidth": 2
          },
          "unit": "short",
          "color": { "mode": "palette-classic" },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "red", "value": 1 }
            ]
          }
        },
        "overrides": []
      },
      "options": {
        "tooltip": { "mode": "multi", "sort": "desc" },
        "legend": { "displayMode": "list", "placement": "bottom" }
      },
      "targets": [
        {
          "expr": "rate(sysmetrics_collector_errors_total{instance=~\"$instance\"}[$__rate_interval])",
          "legendFormat": "{{collector}} errors/s",
          "refId": "A"
        }
      ]
    },

    {
      "collapsed": false,
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 11 },
      "id": 200,
      "title": "CPU",
      "type": "row"
    },
    {
      "id": 10,
      "title": "Overall CPU Utilization %",
      "description": "Percentage of CPU time spent in non-idle modes, aggregated across all selected cores.",
      "type": "timeseries",
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 12 },
      "fieldConfig": {
        "defaults": {
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "smooth",
            "fillOpacity": 30,
            "pointSize": 5,
            "showPoints": "never",
            "spanNulls": false,
            "lineWidth": 2,
            "thresholdsStyle": { "mode": "line+area" }
          },
          "unit": "percentunit",
          "min": 0,
          "max": 1,
          "color": { "mode": "palette-classic" },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "transparent", "value": null },
              { "color": "red", "value": 0.9 }
            ]
          }
        },
        "overrides": []
      },
      "options": {
        "tooltip": { "mode": "multi", "sort": "desc" },
        "legend": { "displayMode": "list", "placement": "bottom" }
      },
      "targets": [
        {
          "expr": "1 - avg(rate(sysmetrics_cpu_seconds_total{mode=\"idle\", instance=~\"$instance\", cpu=~\"$cpu\"}[$__rate_interval]))",
          "legendFormat": "CPU Utilization",
          "refId": "A"
        }
      ]
    },
    {
      "id": 11,
      "title": "CPU Usage by Mode (Stacked)",
      "description": "Breakdown of CPU time across all modes (user, system, iowait, etc.), stacked to show total utilization.",
      "type": "timeseries",
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 12 },
      "fieldConfig": {
        "defaults": {
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "smooth",
            "fillOpacity": 80,
            "pointSize": 5,
            "showPoints": "never",
            "spanNulls": false,
            "stacking": { "mode": "normal", "group": "A" },
            "lineWidth": 1
          },
          "unit": "percentunit",
          "min": 0,
          "color": { "mode": "palette-classic" }
        },
        "overrides": [
          {
            "matcher": { "id": "byName", "options": "idle" },
            "properties": [{ "id": "color", "value": { "mode": "fixed", "fixedColor": "#73BF69" } }]
          },
          {
            "matcher": { "id": "byName", "options": "user" },
            "properties": [{ "id": "color", "value": { "mode": "fixed", "fixedColor": "#FF9830" } }]
          },
          {
            "matcher": { "id": "byName", "options": "system" },
            "properties": [{ "id": "color", "value": { "mode": "fixed", "fixedColor": "#F2495C" } }]
          },
          {
            "matcher": { "id": "byName", "options": "iowait" },
            "properties": [{ "id": "color", "value": { "mode": "fixed", "fixedColor": "#FF6D00" } }]
          },
          {
            "matcher": { "id": "byName", "options": "steal" },
            "properties": [{ "id": "color", "value": { "mode": "fixed", "fixedColor": "#B877D9" } }]
          },
          {
            "matcher": { "id": "byName", "options": "nice" },
            "properties": [{ "id": "color", "value": { "mode": "fixed", "fixedColor": "#5794F2" } }]
          },
          {
            "matcher": { "id": "byName", "options": "irq" },
            "properties": [{ "id": "color", "value": { "mode": "fixed", "fixedColor": "#FADE2A" } }]
          },
          {
            "matcher": { "id": "byName", "options": "softirq" },
            "properties": [{ "id": "color", "value": { "mode": "fixed", "fixedColor": "#C4162A" } }]
          }
        ]
      },
      "options": {
        "tooltip": { "mode": "multi", "sort": "desc" },
        "legend": { "displayMode": "list", "placement": "bottom" }
      },
      "targets": [
        {
          "expr": "avg by (mode) (rate(sysmetrics_cpu_seconds_total{instance=~\"$instance\", cpu=~\"$cpu\"}[$__rate_interval]))",
          "legendFormat": "{{mode}}",
          "refId": "A"
        }
      ]
    },
    {
      "id": 12,
      "title": "Per-Core CPU Utilization %",
      "description": "CPU utilization per individual core (non-idle percentage).",
      "type": "timeseries",
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 20 },
      "fieldConfig": {
        "defaults": {
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "smooth",
            "fillOpacity": 10,
            "pointSize": 5,
            "showPoints": "never",
            "spanNulls": false,
            "lineWidth": 1
          },
          "unit": "percentunit",
          "min": 0,
          "max": 1,
          "color": { "mode": "palette-classic" }
        },
        "overrides": []
      },
      "options": {
        "tooltip": { "mode": "multi", "sort": "desc" },
        "legend": { "displayMode": "table", "placement": "right", "calcs": ["mean", "max"] }
      },
      "targets": [
        {
          "expr": "1 - rate(sysmetrics_cpu_seconds_total{mode=\"idle\", instance=~\"$instance\", cpu=~\"$cpu\"}[$__rate_interval])",
          "legendFormat": "cpu{{cpu}}",
          "refId": "A"
        }
      ]
    },
    {
      "id": 13,
      "title": "CPU User vs System vs IOWait",
      "description": "Comparison of the three most common CPU modes across all cores.",
      "type": "timeseries",
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 20 },
      "fieldConfig": {
        "defaults": {
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "smooth",
            "fillOpacity": 15,
            "pointSize": 5,
            "showPoints": "never",
            "spanNulls": false,
            "lineWidth": 2
          },
          "unit": "percentunit",
          "min": 0,
          "color": { "mode": "palette-classic" }
        },
        "overrides": []
      },
      "options": {
        "tooltip": { "mode": "multi", "sort": "desc" },
        "legend": { "displayMode": "list", "placement": "bottom" }
      },
      "targets": [
        {
          "expr": "avg(rate(sysmetrics_cpu_seconds_total{mode=\"user\", instance=~\"$instance\", cpu=~\"$cpu\"}[$__rate_interval]))",
          "legendFormat": "user",
          "refId": "A"
        },
        {
          "expr": "avg(rate(sysmetrics_cpu_seconds_total{mode=\"system\", instance=~\"$instance\", cpu=~\"$cpu\"}[$__rate_interval]))",
          "legendFormat": "system",
          "refId": "B"
        },
        {
          "expr": "avg(rate(sysmetrics_cpu_seconds_total{mode=\"iowait\", instance=~\"$instance\", cpu=~\"$cpu\"}[$__rate_interval]))",
          "legendFormat": "iowait",
          "refId": "C"
        },
        {
          "expr": "avg(rate(sysmetrics_cpu_seconds_total{mode=\"steal\", instance=~\"$instance\", cpu=~\"$cpu\"}[$__rate_interval]))",
          "legendFormat": "steal",
          "refId": "D"
        }
      ]
    },

    {
      "collapsed": false,
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 28 },
      "id": 300,
      "title": "Memory",
      "type": "row"
    },
    {
      "id": 20,
      "title": "Memory Usage",
      "description": "Current memory usage as a percentage of total memory.",
      "type": "gauge",
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "gridPos": { "h": 6, "w": 4, "x": 0, "y": 29 },
      "fieldConfig": {
        "defaults": {
          "unit": "percentunit",
          "min": 0,
          "max": 1,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 0.7 },
              { "color": "orange", "value": 0.85 },
              { "color": "red", "value": 0.95 }
            ]
          },
          "color": { "mode": "thresholds" }
        },
        "overrides": []
      },
      "options": {
        "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false },
        "showThresholdLabels": false,
        "showThresholdMarkers": true,
        "orientation": "auto"
      },
      "targets": [
        {
          "expr": "1 - (sysmetrics_memory_available_bytes{instance=~\"$instance\"} / sysmetrics_memory_total_bytes{instance=~\"$instance\"})",
          "legendFormat": "{{instance}}",
          "refId": "A",
          "instant": true
        }
      ]
    },
    {
      "id": 21,
      "title": "Memory Breakdown Over Time",
      "description": "Stacked area chart showing how memory is distributed: used, buffers, cached, and free.",
      "type": "timeseries",
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "gridPos": { "h": 8, "w": 12, "x": 4, "y": 29 },
      "fieldConfig": {
        "defaults": {
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "smooth",
            "fillOpacity": 70,
            "pointSize": 5,
            "showPoints": "never",
            "spanNulls": false,
            "stacking": { "mode": "normal", "group": "A" },
            "lineWidth": 1
          },
          "unit": "bytes",
          "min": 0,
          "color": { "mode": "palette-classic" }
        },
        "overrides": [
          {
            "matcher": { "id": "byName", "options": "Used" },
            "properties": [{ "id": "color", "value": { "mode": "fixed", "fixedColor": "#F2495C" } }]
          },
          {
            "matcher": { "id": "byName", "options": "Buffers" },
            "properties": [{ "id": "color", "value": { "mode": "fixed", "fixedColor": "#FF9830" } }]
          },
          {
            "matcher": { "id": "byName", "options": "Cached" },
            "properties": [{ "id": "color", "value": { "mode": "fixed", "fixedColor": "#5794F2" } }]
          },
          {
            "matcher": { "id": "byName", "options": "Free" },
            "properties": [{ "id": "color", "value": { "mode": "fixed", "fixedColor": "#73BF69" } }]
          }
        ]
      },
      "options": {
        "tooltip": { "mode": "multi", "sort": "desc" },
        "legend": { "displayMode": "list", "placement": "bottom", "calcs": ["lastNotNull"] }
      },
      "targets": [
        {
          "expr": "sysmetrics_memory_used_bytes{instance=~\"$instance\"}",
          "legendFormat": "Used",
          "refId": "A"
        },
        {
          "expr": "sysmetrics_memory_buffers_bytes{instance=~\"$instance\"}",
          "legendFormat": "Buffers",
          "refId": "B"
        },
        {
          "expr": "sysmetrics_memory_cached_bytes{instance=~\"$instance\"}",
          "legendFormat": "Cached",
          "refId": "C"
        },
